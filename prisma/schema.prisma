// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario_Web {
  id       Int    @id @default(autoincrement())
  email    String @unique
  senha String

  @@map("usuarios_web")
}

model Usuario {
  id          Int    @id @default(autoincrement())
  nome        String
  localizacao String

  @@map("usuarios")
}
